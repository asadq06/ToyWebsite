@using ToyWebsite.Models
@model User

@if ((bool)Session["GuestUser"] == true)
{
    <li>@Html.ActionLink("Log In","LogIn", "Account", null, new { })</li>
    <li>@Html.ActionLink("Register","Register", "Account", null, new {  })</li>
}
else
{
    <li><a href="#">@Model.userName</a></li>
    <li>@Html.ActionLink("Sign Out", "SignOut", "Account", null, new { })</li>

}
